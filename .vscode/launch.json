{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "test.js",
      "type": "cppdbg",
      "request": "launch",
      "program": "${userHome}/.local/bin/node",
      "args": ["test.js"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/.b",
      "environment": [
        // {
        //   "name": "XXX",
        //   "value": "1"
        // }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build",
    },
  ]
}